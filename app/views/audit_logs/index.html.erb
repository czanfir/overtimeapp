<h1 class="text-center">Audit Log Dashboard</h1>
<br>
<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th>#</th>
    <th>Employee</th>
    <th>Week Starting</th>
    <th>Confirmed At</th>
    <th>Status</th>
  </tr>
  </thead>
  <tbody id="audit_logs">
  <%= render @audit_logs %>
  </tbody>
</table>

<%= render 'shared/pagination', object_to_paginate:@audit_logs %>