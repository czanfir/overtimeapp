<h1 class="text-center">Audit Log Dashboard</h1>
<br>
<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th>#</th>
    <th>Employee</th>
    <th>Week Starting</th>
    <th>Confirmed At</th>
    <th>Status</th>
  </tr>
  </thead>
  <tbody>
  <%= render @audit_logs %>
  </tbody>
</table>

<div class="pagination justify-content-center" >
  <%= paginate @audit_logs %>
</div>